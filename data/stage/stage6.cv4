		if (this.count == 61)
		{
			if (this.Stage_AliveCheck(::stage.TargetList) <= 2)
			{
				this.count++;
				this.world2d.CreateEventFromMap(this.map0.stopEnemy2b.layout, ::stage.ChipObjectSet, this.getroottable());
