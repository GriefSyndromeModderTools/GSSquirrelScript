	this.name = "clearPoint2";
	this.StageObjectCommon();
	::stage.TargetList = [];
	this.SetUpdateFunction(function ()
	{
		if (this.count < 60)
		{
			this.count++;
		}

		if (this.count == 60)
		{
			this.world2d.CreateEventFromMap(this.map0.stopEnemy2.layout, ::stage.ChipObjectSet, this.getroottable());
			this.count++;
