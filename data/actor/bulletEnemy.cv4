function A3_Onpa()
{
	if (this.UpdateStopCheck())
	{
		return false;
	}

	this.sx += this.flag1;
	this.sy = this.sx;
	this.flag1 *= 0.92000002;

	if (this.flag1 < 0.0099999998)
	{
		this.flag1 = 0.0099999998;
	}

	this.count++;

	if (this.count >= 5)
	{
		this.alpha -= 0.050000001;

		if (this.alpha <= 0)
		{
			this.Release();
		}
	}

	return true;
}

