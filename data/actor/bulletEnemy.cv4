function Shot_6A( t )
{
	this.ShotInit(t);
	this.PlaySE(3801);
	this.SetNearTarget();
	this.SetMotion(7050, 0);
	this.u.TotalCount <- 0;
	this.rz = 0.017453292 * this.rand() % 360;
	this.SetEndMotionCallbackFunction(this.EndtoRelease);
	this.hitAction = function ( type_, table_ )
	{
		this.PlaySE(3802);
		this.vx = 0.0;
		this.vy = 0.0;
		this.SetMotion(7050, 2);
	};
	this.SetUpdateFunction(function ()
	{
		if (this.UpdateStopCheck())
		{
			return false;
		}

		this.count++;
		this.u.TotalCount++;

		if (this.u.TotalCount >= 600 || this.hitTarget.len() > 0 || !this.target in this.actor)
		{
			this.vx = 0.0;
			this.vy = 0.0;

			if (this.keyTake <= 1)
			{
				this.PlaySE(3802);
				this.SetMotion(7050, 2);
			}
		}
		else
		{
			switch(this.flag3)
			{
			case 0:
				if (this.count >= 120)
				{
					this.count = 0;

					if ((this.target in this.actor) && this.target != null)
					{
						this.flag2 = this.atan2(this.actor[this.target].y - 40.0 - this.y, this.actor[this.target].x - this.x);
						this.flag3 = 1;
						this.PlaySE(3806);
					}
				}

				break;

			case 1:
				this.flag1 += 0.050000001;

				if (this.flag1 >= 3.0)
				{
					this.flag1 = 3.0;
				}

				if (this.count >= 60)
				{
					this.count = 0;
					this.flag3 = 2;
				}

				break;

			case 2:
				this.flag1 -= 0.1;

				if (this.flag1 < 0.0)
				{
					this.flag1 = 0.0;
				}

				if (this.count >= 60)
				{
					this.count = 0;
					this.flag3 = 0;
				}

				break;
			}

			this.rz += 0.050000001;
			this.vx = this.flag1 * this.cos(this.flag2);
			this.vy = this.flag1 * this.sin(this.flag2);
		}

		return true;
	});
}

function Beam_6A_Core( t )
{
	this.ShotInit(t);
	this.PlaySE(3803);
	this.SetMotion(7051, 0);
	this.u.TotalCount <- 0;
	this.rz = t.rz;
	this.SetEndTakeCallbackFunction(this.KeyActionCheck);
	this.keyAction = [
		null,
		null,
		this.EndtoRelease
	];
	this.SetEndMotionCallbackFunction(this.EndtoRelease);
	this.SetUpdateFunction(function ()
	{
		if (this.UpdateStopCheck())
		{
			return false;
		}

		this.count++;
		this.u.TotalCount++;

		if (this.keyTake <= 1)
		{
			if (this.owner in this.actor)
			{
				local a = this.actor[this.owner];

				if (a.motion == 7023)
				{
					if (this.count == 10)
					{
						local t = this.DefaultShotTable();
						t.rz = this.rz;
						this.option = this.world2d.CreateActor(this.x, this.y, this.direction, this.Beam_6A, t);
					}

					if (this.count >= 100)
					{
						if (this.option)
						{
							this.option.func();
						}

						this.SetMotion(7051, 2);
					}

					if (a.keyTake <= 2)
					{
						this.x = a.point0_x;
						this.y = a.point0_y;

						if (this.option)
						{
							this.option.x = this.x;
							this.option.y = this.y;
						}
					}
					else
					{
						if (this.option)
						{
							this.option.func();
						}

						this.SetMotion(7051, 2);
					}
				}
				else
				{
					if (this.option)
					{
						this.option.func();
					}

					this.SetMotion(7051, 2);
				}
			}
		}

		return true;
	});
}

function Beam_6A( t )
{
	this.ShotInit(t);
	this.SetMotion(7051, 3);
	this.priority = 250;
	this.u.TotalCount <- 0;
	this.rz = t.rz;
	this.SetEndMotionCallbackFunction(this.EndtoRelease);
	this.func = function ()
	{
		this.SetMotion(7051, 5);
		this.func = null;
	};
	this.SetUpdateFunction(function ()
	{
		if (this.UpdateStopCheck())
		{
			return false;
		}

		this.count++;

		if (this.keyTake == 3)
		{
			if (this.count >= 60)
			{
				this.PlaySE(3804);
				this.SetMotion(7051, 4);
			}
		}

		return true;
	});
}

function Shot_6B( t )
{
	this.ShotInit(t);
	this.PlaySE(3801);
	this.SetMotion(7150, 0);
	this.u.TotalCount <- 0;
	this.rz = 0.017453292 * this.rand() % 360;
	this.vx = 3.0 * this.direction;
	this.vy = 0.0;
	this.hitAction = function ( type_, table_ )
	{
		this.PlaySE(3802);
		this.vx = 0.0;
		this.vy = 0.0;
		this.SetMotion(7150, 2);
	};
	this.SetEndMotionCallbackFunction(this.EndtoRelease);
	this.func = function ()
	{
		this.PlaySE(3802);
		this.vx = 0.0;
		this.vy = 0.0;
		this.SetMotion(7150, 2);
	};
	this.SetUpdateFunction(function ()
	{
		if (this.UpdateStopCheck())
		{
			return false;
		}

		this.count++;
		this.u.TotalCount++;

		if (this.u.TotalCount >= 600 || this.hitTarget.len() > 0 || !this.target in this.actor)
		{
			if (this.keyTake <= 1)
			{
				this.func();
			}
		}

		return true;
	});
}

function Trail_6B_Assult( t )
{
	this.ShotInit(t);
	this.SetMotion(7151, 0);
	this.rz = 0.017453292 * this.rand() % 360;
	this.vx = -1.0 + this.rand() % 21 * 0.1;
	this.vy = -1.0;
	this.SetUpdateFunction(function ()
	{
		if (this.UpdateStopCheck())
		{
			return false;
		}

		this.vy += 0.34999999;
		this.sx *= 0.98000002;
		this.sy = this.sx;

		if (this.sx <= 0.0099999998)
		{
			this.Release();
		}

		return true;
	});
}

